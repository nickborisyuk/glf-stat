{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "builds": [
    {
      "src": "server/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server/src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
